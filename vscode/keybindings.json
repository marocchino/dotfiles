[
    {
        "key": "ctrl+cmd+k",
        "command": "-extension.macDictionary.lookup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+[",
        "command": "vscode-neovim.escape"
    },
    {
        "key": "ctrl+c",
        "command": "-vscode-neovim.escape"
    },
    {
        "key": "ctrl+b",
        "command": "-cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "t ctrl+a",
        "command": "workbench.action.tasks.runTask",
        "args": "test",
        "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "t ctrl+f",
        "command": "workbench.action.tasks.runTask",
        "args": "test one file",
        "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "t ctrl+n",
        "command": "workbench.action.tasks.runTask",
        "args": "test nearby",
        "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "ctrl+cmd+k",
        "command": "-extension.macDictionary.lookup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+[",
        "command": "vscode-neovim.escape"
    },
    {
        "key": "ctrl+c",
        "command": "-vscode-neovim.escape"
    },
    {
        "key": "ctrl+b",
        "command": "-cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+b",
        "command": "-vscode-neovim.ctrl-b",
        "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "ctrl+b",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "cmd+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "shift+cmd+d",
        "command": "-workbench.view.debug"
    },
    {
        "key": "ctrl+d",
        "command": "-deleteRight",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+d",
        "command": "-editor.action.outdentLines",
        "when": "editorTextFocus && neovim.ctrlKeysInsert && !neovim.recording && neovim.mode == 'insert'"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "ctrl+d",
        "command": "-vscode-neovim.ctrl-d",
        "when": "editorTextFocus && !editorReadonly && neovim.mode != 'insert'"
    },
    {
        "key": "alt+f8",
        "command": "-editor.action.marker.next",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "shift+cmd+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.terminal.focus",
        "when": "editorTextFocus"
    },
    {
        "key": "f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+left",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+[",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+right",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+]",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+t",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "shift+cmd+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusSideBar",
        "when": "terminalFocus || editorTextFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "sideBarFocus || textInputFocus"
    },
    {
        "key": "cmd+0",
        "command": "-workbench.action.focusSideBar"
    },
    {
        "key": "ctrl+p",
        "command": "insertSnippet",
        "when": "editorTextFocus && hasSnippetCompletions && !editorTabMovesFocus && !inSnippetMode && neovim.init && neovim.mode == 'insert'"
    },
    {
        "key": "cmd+k cmd+right",
        "command": "-workbench.action.focusRightGroup"
    },
    {
        "key": "cmd+k cmd+left",
        "command": "-workbench.action.focusLeftGroup"
    },
    {
        "key": "] q",
        "command": "search.action.focusNextSearchResult",
        "when": "hasSearchResult && editorTextFocus && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "f4",
        "command": "-search.action.focusNextSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "[ q",
        "command": "search.action.focusPreviousSearchResult",
        "when": "hasSearchResult && editorTextFocus && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "shift+f4",
        "command": "-search.action.focusPreviousSearchResult",
        "when": "hasSearchResult"
    }
]